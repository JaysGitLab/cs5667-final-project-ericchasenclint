    "http-status": "^1.0.1",
    "ie-shim": "^0.1.0",
    "innograph": "git://github.com/linnovate/innograph.git#devel",
    "joi": "^10.5.2",
    "jquery": "^3.3.1",
    "material": "^0.1.1",
    "method-override": "^2.3.9",
    "mongoose": "^4.10.6",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.14.3",
    "preboot": "^5.0.0-rc.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "reflect-metadata": "^0.1.12",
